  "tokenColors": [
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#f1f1f1" // e5e5eb
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#79808d"
      }
    },
    {
      "name": "Keywords and control flow",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.directive.using",
        "keyword.other.operator",
        "entity.name.operator"
      ],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#44dd99"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "meta.embedded.line.js",
      ],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "String Templates Brackets",
      "scope": [
        "meta.embedded.line.js",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
      ],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal"
      ],
      "settings": {
        "foreground": "#67e8f9"
      }
    },
    {
      "name": "Class",
      "scope": [
        "support.class",
        "support.type",
      ],
      "settings": {
        "foreground": "#ff4488" // d53f67
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "entity.name.type",
        "entity.name.namespace",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#0fcdae" // d53f67
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
